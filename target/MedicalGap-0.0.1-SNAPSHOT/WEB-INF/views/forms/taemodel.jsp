<style>
.col-md-5.col-xs-10{
	margin-bottom: 15px !important;
}

</style>

<div class="modal fade" id="myModalT" role="dialog">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
			</div>
			<div class="modal-body">
				<div class="row">
					<div class="col-xs-12 col-md-6">

						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Date :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input type="date" class="form-control"
									id="dateMdl" readonly/>
							</div>
						</div>

						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label" style="margin-bottom: 2px;">Nombre de
						publications :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input type="number" class="form-control"
									id="nbrePubBcMdl"readonly />
							</div>
						</div>

						<div class="form-group">

							<label class="col-md-4 col-xs-10 control-label" style="margin-bottom: 2px;">Nombre de
								publications Total :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 4px;">
								<input type="number" class="form-control"
									id="nbreTotMdl"  readonly/>
							</div>
						</div>

						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Titre HCP :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input type="text" class="form-control"
									id="titreHcpMdl" readonly/>
							</div>
						</div>
						
						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Type TAE :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input type="text" class="form-control"
									id="typeTaeMdl" readonly/>
							</div>
						</div>
						<div class="form-group">

							<label class="col-md-4 col-xs-10 control-label">Essai clinique :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input  type="text"
									class="form-control" id="essaiCliniqueMdl"
									readonly/>
							</div>
						</div>
					</div>
					<div class="col-xs-12 col-md-6">


						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Proprietaire :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input  type="text" class="form-control"
									id="proprietaireMdl" readonly/>
							</div>
						</div>

						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Conclusion :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<textarea rows="4" cols="70" class="form-control"
									id="conclusionMdl" readonly></textarea>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Gaps, Insight ou
								Recommendation :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<textarea rows="4" cols="90" class="form-control"
									id="gapsInsightRecoMdl" readonly></textarea>
							</div>
						</div>
						<div class="form-group">
							<label class="col-md-4 col-xs-10 control-label">Responsable :</label>
							<div class="col-md-5 col-xs-10" style="margin-bottom: 2px;">
								<input type="text" class="form-control" id="responsableMdl"
									readonly />
							</div>
						</div>

					</div>
				</div>
				</div>
				
			</div>
		</div>
	</div>